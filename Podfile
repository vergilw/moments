  pod 'Kingfisher'
  pod 'MLeaksFinder'
  
  pod 'YYKit'
  pod 'MJRefresh'
  pod 'UITableView+FDTemplateLayoutCell'
  pod 'UITextView+Placeholder'
  pod 'MBProgressHUD'


  target 'MomentsUITests' do
    # Pods for testing
  end

end
